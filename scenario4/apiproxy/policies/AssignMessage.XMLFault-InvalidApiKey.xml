<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage name="AssignMessage.XMLFault-InvalidApiKey">
    <DisplayName>AssignMessage.XMLFault-InvalidApiKey</DisplayName>
    <AssignTo createNew="false" transport="http" type="response"/>
    <Set>
        <Payload contentType="text/xml" variablePrefix="{" variableSuffix="}">
            <Fault>
                <faultcode>400</faultcode>
                <faultstring>Invalid Request</faultstring>
                <detail>The Request was invalid</detail>
            </Fault>
        </Payload>
        <StatusCode>400</StatusCode>
        <ReasonPhrase>Bad Request</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
</AssignMessage>