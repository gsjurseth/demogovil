<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XSL name="XSLT-Paginate">
    <DisplayName>XSLT-Paginate</DisplayName>
    <Parameters ignoreUnresolvedVariables="false">
        <Parameter name="offset" ref="page"/>
        <Parameter name="limit" ref="maxpage"/>
    </Parameters>
    <Source>response</Source>
    <ResourceURL>xsl://paginate.xslt</ResourceURL>
    <OutputVariable>paginatedResponse</OutputVariable>
</XSL>