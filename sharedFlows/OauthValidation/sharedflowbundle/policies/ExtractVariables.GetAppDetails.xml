        <Variable name="department" type="string">
            <XPath>/App/Attributes/Attribute[Name='department']/Value/text()</XPath>
        </Variable>
        <Variable name="role" type="string">
            <XPath>/App/Attributes/Attribute[Name='role']/Value/text()</XPath>
        </Variable>
        <Variable name="name" type="string">
            <XPath>/App/Attributes/Attribute[Name='Name']/Value/text()</XPath>
        </Variable>
    </XMLPayload>
</ExtractVariables>